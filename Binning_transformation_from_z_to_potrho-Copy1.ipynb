   "source": [
    "## load daily vhrho since ty_trans doesn't have daily data\n",
    "\n",
    "vhrho = cc.querying.getvar(expt,'vhrho_nt', session, start_time = start_time, end_time = end_time)\n",
    "vhrho = vhrho.sel(time = time_slice).sel(yt_ocean = -35, method = 'nearest')\n",
    "\n",
    "# vhrho is outputted as being on t grid, but refers to transport on northern edge (a u-cell latitude)\n",
    "\n",
    "# to convert vhrho into ty_trans, use vhrho*dxu. However, on different grids so need to play around a bit with renaming\n",
    "\n",
    "# t-cells are further south and west than u-cells\n",
    "## some grid data is required, a little complicated because these variables don't behave well with some\n",
    "dxu = cc.querying.getvar(expt, 'dxu',session, n=1, ncfile = 'ocean_grid.nc')\n",
    "dxu = dxu.sel(yu_ocean = -35, method = 'nearest')\n",
    "xt_ocean = cc.querying.getvar(expt, 'xt_ocean',session, n=1, ncfile = 'ocean_grid.nc')\n",
    "\n",
    "dxu.coords['xu_ocean'] = xt_ocean.values # does't matter since just shifting in lon, u cell width is same as northern width of t-cell\n",
    "dxu = dxu.rename({'xu_ocean':'xt_ocean'})\n",
    "\n",
    "ty_trans_from_vh = vhrho*dxu.values\n",
    "ty_trans_from_vh\n",
    "## take annual average because I don't have daily potrho2\n"
   ]
  },
