   "source": [
    "%%time\n",
    "pot_rho_2 = cc.querying.getvar(expt,'pot_rho_2', session, start_time = start_time, end_time = end_time)\n",
    "pot_rho_2 = pot_rho_2.sel(time = time_slice).sel(yt_ocean = lat_range)\n",
    "\n",
    "## choose month 0\n",
    "pot_rho_2 = pot_rho_2.isel(time = 0)\n",
    "# month_length = pot_rho_2.time.dt.days_in_month\n",
    "# pot_rho_2 = (pot_rho_2*month_length).sum('time')/365\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "00cb7a08-4c4a-4742-ad36-6db0e648f434",
   "metadata": {},
   "source": [
    "Create an `xgcm` grid for interpolation, and then interpolate density onto the meridional transport grid"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 62,
   "id": "eb10c7cd-02e8-4bb5-952e-a4e081606a78",
   "metadata": {},
   "outputs": [],
   "source": [
    "ds = xr.Dataset({'ty_trans':ty_trans,'pot_rho_2':pot_rho_2}) \n",
    "grid = xgcm.Grid(ds, coords={'Y':{'center':'yt_ocean','right':'yu_ocean'}},periodic = False)\n",
    "\n",
    "# interpolate density (t-grid) to ty_trans grid (xt_ocean and yu_ocean)\n",
    "pot_rho_2 = grid.interp(pot_rho_2,'Y',boundary='extend')\n",
    "pot_rho_2_35 = pot_rho_2.sel(yu_ocean = -35, method = 'nearest')\n"
   ]
  },
